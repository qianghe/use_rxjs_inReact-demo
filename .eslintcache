   }\n    case ActionTypes.SET_BRANDS: {\n      const brands = action.payload || []\n      return {\n        ...state,\n        brands\n      }\n    }\n    default:\n      return state\n  }\n}\n\nexport default queriesReducer\n\n// 请求品牌数据\nexport const fetchBrandsEpic = (action$, state$) => {\n  return action$.pipe(\n    ofType(ActionTypes.QUERY_BRANDS),\n    switchMap(() => getAxiosObservable(QUERY_BRANDS_URL).pipe(\n      mergeMap(response => of(\n        updateBrands(_.get(response, 'brands', [])),\n        updateQueries({\n          brandId: _.get(response, 'brands.0.brandId')\n        })\n      )),\n      catchError(error => of({\n        type: ActionTypes.FETCH_ERROR,\n        payload: error.statusText\n      }))\n    ))\n  )\n}\n\n// 请求商品数据\nexport const fetchByQueriesEpic = (action$, state$) => {\n  const stopPolling$ = action$.ofType(ActionTypes.POLLING_STOP)\n  const paramsChange$ = state$.pipe(\n    map(({ queries }) => {\n      const { brands, sorter, pager, filters, ...restProps } = queries\n      const { pageSize = 20, pageNum = 1} = pager\n      const { sortKey, sortOrder = 'asc' } = sorter\n      return {\n        measureCode: filters.join(','),\n        pageSize,\n        pageNum,\n        sortField: sortKey,\n        sortType: sortOrder,\n        ...restProps,\n        ...defaultProps\n      }\n    }),\n    distinctUntilChanged(_.isEqual),\n    filter(params => !_.isEmpty(_.get(params, 'brandId')))\n  )\n  // 操作数据流\n  return action$.pipe(\n    ofType(ActionTypes.POLLING_START),\n    combineLatest(paramsChange$, (action, params) => params),\n    switchMap((params) => {\n      const polling$ = interval(5 * 60 * 1000).pipe(\n        takeUntil(stopPolling$),\n        startWith(null),\n        switchMap(() => getAxiosObservable(QUERY_PRODUCTS_URL, {\n          params\n        }).pipe(\n          mergeMap(data => of(updateProducts(data))),\n          startWith(startFetch()),\n          catchError(error => of({\n            type: ActionTypes.FETCH_ERROR,\n            payload: error.statusText\n          }))\n        ))\n      )\n      return polling$\n    })\n  )\n}\n\n","/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/components/Content.jsx",[],"/Users/heqiang/demos/rxjs-demo/src/api/index.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/config/table.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/config/render.js",[],"/Users/heqiang/demos/rxjs-demo/src/utils/format.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/components/Cells/TrendUnit.jsx",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/config/index.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/components/Cells/ItemInfo.jsx",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/components/Picker.jsx",[],"/Users/heqiang/demos/rxjs-demo/src/api/base.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/actions/index.js",[],"/Users/heqiang/demos/rxjs-demo/src/pages/OnlineDemo/actions/product.js",[],{"ruleId":"228","replacedBy":"229"},{"ruleId":"230","replacedBy":"231"},{"ruleId":"232","severity":1,"message":"233","line":7,"column":14,"nodeType":"234","messageId":"235","endLine":7,"endColumn":24},{"ruleId":"232","severity":1,"message":"236","line":5,"column":64,"nodeType":"234","messageId":"235","endLine":5,"endColumn":67},"no-native-reassign",["237"],"no-negated-in-lhs",["238"],"no-unused-vars","'exhaustMap' is defined but never used.","Identifier","unusedVar","'tap' is defined but never used.","no-global-assign","no-unsafe-negation"]